<h1>user edit page</h1>
<%= form_for @user, method: "put" do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %><br/>
  <%= f.label :email %>
  <%= f.text_field :email %><br/>
  <%= f.label :phone_number %>
  <%= f.text_field :phone_number%><br><br>
  <%= f.label :avatar, 'Profile Picture' %>
  <%= f.file_field :avatar %>
  <%= f.submit %>
<% end %>

<h1>Vendor Edit Page</h1>
<%= form_for @vendor, method: "put" do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %><br/>
  <%= f.label :email %>
  <%= f.text_field :email %><br/>
  <%= f.label :phone_number %>
  <%= f.text_field :phone_number %><br/>
  <%= f.label :address %>
  <%= f.text_field :address %><br/>
  <%= f.label :bio %>
  <%= f.text_field :bio %><br><br>
  <%= f.submit %>
<% end %>
<%= link_to 'destroy vendor', vendor_path, method: 'delete' %>